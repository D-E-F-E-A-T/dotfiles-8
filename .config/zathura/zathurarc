# zathurarc-darkbrown


####### General Settings ############
set adjust_open width
set width 636
set height 382
set font "tewi 8"
set show_inputbar false


###### Colorscheme ############
set default-bg                  "#020202" #00
set default-fg                  "#93a1a1" #07

set statusbar-fg                "#d0d0d0" #07
set statusbar-bg                "#080808" #00

set inputbar-bg                 "#0d0c0c" #00 currently not used
set inputbar-fg                 "#808080" #02

set notification-error-bg       "#5c3929" #08
set notification-error-fg       "#fdf6e3" #00

set notification-warning-bg     "#3d4251" #08
set notification-warning-fg     "#fdf6e3" #00

set highlight-color             "#4b483d" #0A
set highlight-active-color      "#fdf6e3" #0D

set completion-highlight-fg     "#4b483d" #02
set completion-highlight-bg     "#75B5AA" #0C

set completion-bg               "#303030" #02
set completion-fg               "#75B5AA" #0C

set notification-bg             "#2d2a2a" #0B
set notification-fg             "#000003" #00

set recolor-lightcolor          "#020202" #00
set recolor-darkcolor           "#8b8b8b" 
set recolor                     "true"

# setting recolor-keep true will keep any color your pdf has.
# if it is false, it'll just be black and white
set recolor-keephue             "false"

# Rest of your configs go here:


