set font                        "Cantarell 8"
set default-bg                  "#080a11" #00
set default-fg                  "#d3dde8" #01

set statusbar-fg                "#d3dde8"    #04
set statusbar-bg                "#0c0e17" #01

set inputbar-bg                 "#080808" #00 currently not used
set inputbar-fg                 "#404040" #02

set notification-error-bg       "#4ec1c1" #08
set notification-error-fg       "#d3dde8" #00

set notification-warning-bg     "#484f59" #08
set notification-warning-fg     "#d3dde8" #00

set highlight-color             "#3d4550" #0A
set highlight-active-color      "#d3dde8" #0D

set completion-highlight-fg     "#ededf9"  #02
set completion-highlight-bg     "#0caaab" #0C

set completion-bg               "#080a11" #02
set completion-fg               "#d3dde8" #0C

set notification-bg             "#6ba5bd" #0B
set notification-fg             "#d3dde8" #00

set recolor-lightcolor          "#080a11" #00
set recolor-darkcolor           "#d3dde8" #06
set recolor                     "true"

# setting recolor-keep true will keep any color your pdf has.
# if it is false, itll just be black and white
set recolor-keephue             "false"

